	</init-param>
  </servlet>
  
  <servlet>
  	<servlet-name>MyCity</servlet-name>
  	<servlet-class>com.javasoft.libs.views.MyServlet1</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>MyCity</servlet-name>
  	<url-pattern>/servlet/MyCity1</url-pattern>
  </servlet-mapping>
  
  
  
  
  
  
</web-app>
